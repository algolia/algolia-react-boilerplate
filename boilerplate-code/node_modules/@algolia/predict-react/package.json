{
  "name": "@algolia/predict-react",
  "description": "Predict UI for React",
  "version": "1.4.0",
  "license": "MIT",
  "homepage": "https://github.com/algolia/predict",
  "repository": "algolia/predict",
  "author": {
    "name": "Algolia, Inc.",
    "url": "https://www.algolia.com"
  },
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/umd/index.js",
  "module": "./dist/esm/index.js",
  "umd:main": "./dist/umd/index.js",
  "unpkg": "./dist/umd/index.js",
  "jsdelivr": "./dist/umd/index.js",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "module": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/umd/index.js",
      "umd": "./dist/umd/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:esm": "babel src --root-mode upward --extensions '.ts,.tsx' --out-dir dist/esm --ignore '**/*/__tests__/'",
    "build:types": "tsc -p ./tsconfig.declaration.json --outDir ./dist/esm",
    "build:umd": "rollup --config",
    "build": "yarn build:clean && yarn build:esm && yarn build:umd && yarn build:types",
    "dev": "chokidar \"src/**/*.ts\" \"src/**/*.tsx\" --ignore \"**/*/__tests__/**/*\" --command \"yarn run prepare\" --initial",
    "prepare": "yarn build:esm && yarn build:types"
  },
  "dependencies": {
    "@algolia/predict-core": "1.4.0",
    "dequal": "^2.0.0",
    "use-sync-external-store": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">= 16.8.0 < 19",
    "react-dom": ">= 16.8.0 < 19"
  }
}
