{
  "name": "algolia-react-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for Algolia Demos",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run format"
    }
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite --host",
    "preview": "vite preview --host",
    "format": "prettier --write .",
    "test": "npm run lint",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algolia/algolia-react-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/algolia/algolia-react-boilerplate/issues"
  },
  "homepage": "https://github.com/algolia/algolia-react-boilerplate#readme",
  "dependencies": {
    "@algolia/predict": "1.0.0-alpha.26",
    "@algolia/predict-react": "^1.3.0",
    "@algolia/recommend": "^4.12.2",
    "@algolia/recommend-react": "^1.4.0",
    "@algolia/ui-components-horizontal-slider-react": "^1.0.0",
    "@algolia/ui-components-horizontal-slider-theme": "^1.0.0",
    "@rollup/plugin-inject": "^5.0.2",
    "@vitejs/plugin-react": "^2.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "algoliasearch": "4.14.2",
    "framer-motion": "^7.0.0",
    "i18next": "^21.8.11",
    "lodash": "^4.17.21",
    "npm": "^8.5.5",
    "rc-slider": "^10.0.1",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.4",
    "react-instantsearch-hooks-web": "^6.25.0",
    "react-router-dom": "6.3.0",
    "recoil": "^0.7.4",
    "sass": "^1.49.0",
    "search-insights": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
