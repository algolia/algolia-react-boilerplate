{
  "name": "@algolia/predict-core",
  "description": "JavaScript primitives for Predict libraries.",
  "version": "1.6.1",
  "license": "MIT",
  "homepage": "https://github.com/algolia/predict",
  "repository": "algolia/predict",
  "author": {
    "name": "Algolia, Inc.",
    "url": "https://www.algolia.com"
  },
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/umd/index.production.js",
  "module": "./dist/esm/index.js",
  "umd:main": "./dist/umd/index.production.js",
  "unpkg": "./dist/umd/index.production.js",
  "jsdelivr": "./dist/umd/index.production.js",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "module": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/umd/index.production.js",
      "umd": "./dist/umd/index.production.js"
    },
    "./src/tools": "./src/tools/index.ts"
  },
  "sideEffects": false,
  "files": [
    "dist/",
    "src/tools/"
  ],
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:esm": "babel src --root-mode upward --extensions '.ts,.tsx' --out-dir dist/esm --ignore '**/*/__tests__/'",
    "build:types": "tsc -p ./tsconfig.declaration.json --outDir ./dist/esm",
    "build:umd": "rollup --config",
    "build": "yarn build:clean && yarn build:esm && yarn build:umd && yarn build:types",
    "prepare": "yarn build:esm && yarn build:types"
  },
  "devDependencies": {
    "@algolia/predict": "^1.0.0-alpha.21"
  }
}
